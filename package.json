    "next": "12.2.0",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.4.0",
    "nextjs-progressbar": "^0.0.14",
    "qs": "^6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-player": "^2.11.0",
    "react-query": "^3.39.1",
    "react-use": "^17.4.0",
    "swiper": "^8.3.2",
    "zustand": "^4.0.0-rc.1"
